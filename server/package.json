{
  "name": "crypto-folio",
  "version": "1.0.0",
  "description": "Crypto Folio app",
  "main": "server.js",
  "scripts": {
    "start:dev": "tsx --watch src/server.ts",
    "start": "node server.js",
    "test:create-prisma-environment": "npm link ./prisma/vitest-environment-prisma",
    "test:install-prisma-environment": "npm link vitest-environment-prisma",
    "build": "npm install && tsup src !src/**/*.spec.* --out-dir build && cp package.json ./build",
    "postbuild": "rm -rf node_modules && npm ci --omit=dev && mv node_modules/ ./build",
    "test": "vitest run --dir src/use-cases",
    "pretest:e2e": "run-s test:create-prisma-environment test:install-prisma-environment",
    "test:e2e": "vitest run --dir src/http"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@rocketseat/eslint-config": "^2.1.0",
    "@types/bcryptjs": "2.4.4",
    "@types/multer": "1.4.8",
    "@types/node": "^20.6.2",
    "@types/supertest": "2.0.14",
    "eslint": "^8.49.0",
    "npm-run-all": "4.1.5",
    "prisma": "5.3.1",
    "supertest": "6.3.3",
    "tsup": "^7.2.0",
    "tsx": "^3.12.10",
    "typescript": "^5.2.2",
    "vitest": "0.34.4"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "3.427.0",
    "@fastify/cookie": "9.1.0",
    "@fastify/cors": "8.4.0",
    "@fastify/formbody": "7.4.0",
    "@fastify/jwt": "7.2.1",
    "@fastify/multipart": "8.0.0",
    "@prisma/client": "5.3.1",
    "axios": "1.5.1",
    "bcryptjs": "2.4.3",
    "dotenv": "16.3.1",
    "fastify": "^4.23.2",
    "vite-tsconfig-paths": "4.2.1",
    "zod": "3.22.2"
  }
}
